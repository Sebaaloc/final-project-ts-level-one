{
    "openapi": "3.0.3",
    "info": {
      "title": "Swagger EPS",
      "description": "This is a swagger for documenting the overall EPS project for Typescript N1",
      "contact": {
        "email": "sebastian.alarcon@infocenter.com"
      },
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "https://localhost:3000"
      }
    ],
    "tags": [
      {
        "name": "affiliate",
        "description": "Crud operations for affiliates"
      },
      {
        "name": "medic",
        "description": "Crud operations for medics"
      }
    ],
    "paths": {
      "/affiliate": {
        "put": {
          "tags": [
            "affiliate"
          ],
          "summary": "Update an existing affiliate",
          "description": "Update an existing affiliate by Id",
          "operationId": "updateAffiliate",
          "requestBody": {
            "description": "Update an existent affiliate in the store",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/affiliate"
                }
              }
            },
            "required": true
          },
          "responses": {
            "200": {
              "description": "Successful operation",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/affiliate"
                  }
                }
              }
            },
            "400": {
              "description": "Client error",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "message": {
                        "type": "string",
                        "description": "Error message",
                        "example": "There was an error"
                      }
                    }
                  }
                },
            "404": {
              "description": "Not found error",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "message": {
                        "type": "string",
                        "description": "Error message",
                        "example": "User: 123456789 does not exist"
                      }
                    }
                  }
                }
              }
            },
            "500": {
              "description": "Internal server error",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "message": {
                        "type": "string",
                        "description": "Error message",
                        "example": "Internal server error"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
        "schemas": {
            "affiliate": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "Name of the affiliate",
                        "example": "Sebastian",
                        "required": true
                    },
                    "middleName": {
                        "type": "string",
                        "description": "Middle name of the affiliate",
                        "example": "David",
                        "required": false
                    },
                    "lastNames": {
                        "type": "string",
                        "description": "Lastnames of the affiliate",
                        "example": "Marin",
                        "required": true
                    },
                    "birthdate": {
                        "type": "string",
                        "description": "Birthdate of affiliate",
                        "example": "2023-11-03T01:03:30.780Z",
                        "required": true
                    },
                    "identificationType": {
                        "type": "string",
                        "description": "Type of identification",
                        "example": "passport",
                        "required": true,
                        "enum": [
                            "citizenId",
                            "foreignId",
                            "passport",
                            "identityCard"
                        ]
                    },
                    "id": {
                        "type": "string",
                        "description": "Id of affiliate",
                        "example": "123456789",
                        "required": true
                    },
                    "phoneNumber": {
                        "type": "string",
                        "description": "Phone number of affiliate",
                        "example": "5555555",
                        "required": false
                    },
                    "cellPhoneNumber": {
                        "type": "string",
                        "description": "Cellphone number of affiliate",
                        "example": "2023-11-03T01:03:30.780Z",
                        "required": true
                    },
                    "postalCode": {
                        "type": "string",
                        "description": "Postal code of affiliate",
                        "example": "01005",
                        "required": true
                    },
                    "address": {
                        "type": "string",
                        "description": "Address of affiliate",
                        "example": "Cra algo",
                        "required": true
                    },
                    "country": {
                        "type": "string",
                        "description": "Country of affiliate",
                        "example": "Colombia",
                        "required": true
                    },
                    "city": {
                        "type": "string",
                        "description": "City of affiliate",
                        "example": "Medellin",
                        "required": true
                    },
                    "email": {
                        "type": "string",
                        "description": "Email of affiliate",
                        "example": "test@email.com",
                        "required": true
                    },
                    "gender": {
                        "type": "string",
                        "description": "Gender of affiliate",
                        "example": "male",
                        "required": true,
                        "enum": [
                            "male",
                            "female",
                            "other"
                        ]
                    },
                    "status": {
                        "type": "string",
                        "description": "Status of affiliate",
                        "example": "male",
                        "required": true,
                        "enum": [
                            "ACTIVE",
                            "DISABLED"
                        ]
                    },
                    "socialStratum": {
                        "type": "number",
                        "description": "Social stratum of affiliate",
                        "example": 4
                    },
                    "sisben": {
                        "type": "boolean",
                        "description": "Does the affiliate have sisben",
                        "example": "test@email.com",
                        "required": true
                    },
                    "requiresCompany": {
                        "type": "boolean",
                        "description": "Does the affiliate need company",
                        "example": "test@email.com",
                        "required": true
                    }
                }
            }
        }
    }
  }